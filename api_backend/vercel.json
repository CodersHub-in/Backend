{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**/*.ts"],
                "excludeFiles": ["node_modules/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.ts"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 10
        }
    }
}
